	<queryString>
		<![CDATA[SELECT
     A.*,
     PI.*,
     P.*,
     c1.`full_name` AS c1_fn,
     c1.`email` AS c1_e,
     c1.`mobile_no` AS c1_mn,
     c1.`house_tel_no` AS c1_htn,
     c1.`title` AS c1_t,
     c1.`identity_type` AS c1_it,
     c1.`office_tel_no` AS c1_otn,
     c1.`date_of_birth` AS c1_dob,
     c1.`identity_no` AS c1_in,
     c1.`fax_no` AS c1_fax,
     PM.`description` AS pm_desc,
     BP.`company_name` AS BP_name,
     proptype.`description` AS proptype_desc,
     addr.`address_line_1` AS c1_l1,
     addr.`address_line_2` AS c1_l2,
     addr.`address_line_3` AS c1_l3,
     addr.`address_line_4` AS c1_l4,
     addr.`postcode` AS c1_pc,
     addr.`city` AS c1_city,
     addr.`state` AS c1_state,
     addr.`country` AS c1_country,
     code_det.`description` AS c1title,
     code_det_A.`description` AS c1state,
     code_det_B.`description` AS c1country,
     c1.`bumi_indicator` AS c1bumi,
     c1.`contact_Person` AS c1ctc,
     c1.`customer_category` AS c1cust_cat
FROM
     `account` A INNER JOIN `project_inventory` PI ON A.`INVENTORY_ID` = PI.`INVENTORY_ID`
     INNER JOIN `project` P ON PI.`PROJECT_ID` = P.`PROJECT_ID`
     INNER JOIN `business_partner` BP ON P.`developer_id` = BP.`partner_id`
     INNER JOIN `code_det` proptype ON P.`property_type` = proptype.`code`
     LEFT OUTER JOIN `customer` c1 ON A.`customer_id` = c1.`customer_id`
     RIGHT OUTER JOIN `code_det` PM ON A.`book_pymt_method` = PM.`code`
     INNER JOIN `address` addr ON c1.`address_id` = addr.`address_id`
     INNER JOIN `code_det` code_det ON c1.`title` = code_det.`code`
     INNER JOIN `code_det` code_det_A ON addr.`state` = code_det_A.`code`
     INNER JOIN `code_det` code_det_B ON addr.`country` = code_det_B.`code`
WHERE
     A.ACCOUNT_ID = $P{account_id}]]>
