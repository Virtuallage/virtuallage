	<queryString>
		<![CDATA[SELECT A.*,PI.*,P.PROJECT_NAME,I.INSTITUTION_NAME,PM.description as pm_description,BB.description as bb_description
FROM account A
INNER JOIN project_inventory PI ON A.INVENTORY_ID = PI.INVENTORY_ID
INNER JOIN project P ON P.PROJECT_ID = PI.PROJECT_ID
INNER JOIN institution I ON P.INSTITUTION_ID = I.INSTITUTION_ID
LEFT OUTER JOIN code_det PM on PM.code = A.book_pymt_method
LEFT OUTER JOIN code_det BB on BB.code = A.book_pymt_bank
WHERE A.ACCOUNT_ID = $P{account_id}
AND PM.code_header_id = 'PAYM']]>
	</queryString>
